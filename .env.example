# Example .env file for projects - Claude Code can read automatically
# Copy to .env and fill in your actual keys

# Development Environment
NODE_ENV=development
PORT=3000
HOST=localhost

# API Keys (Claude Code has autonomous access)
API_KEY=${OPENAI_API_KEY}
GITHUB_TOKEN=${GITHUB_TOKEN}
DATABASE_URL=${DATABASE_URL}

# Project-specific keys
PROJECT_API_KEY=your-project-key
PROJECT_SECRET=your-project-secret

# Feature Flags
ENABLE_AUTONOMOUS_MODE=true
CLAUDE_AUTO_ACCESS=true
SKIP_API_PROMPTS=true